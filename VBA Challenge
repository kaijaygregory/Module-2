Sub tickerloop()
    
    'Set a variable for holding the ticker symbol
    Dim tickersymbol As String
    
    'Set a variable for holding the total stock volume
    Dim tickervolume As Double
    
    'Keep track of the location for each ticker name in the summary table
    Dim summary_ticker_row As Long
    
    'Set a variable for opening price
    Dim opening_price As Double
    
    'Set variables for closing price, yearly change, and percent change
    Dim closing_price As Double
    Dim yearly_change As Double
    Dim percent_change As Double
    
    For Each ws In Worksheets
    tickervolume = 0
    summary_ticker_row = 2
    opening_price = ws.Cells(2, 3).Value
     
    'Label the headers for the summary table
    ws.Cells(1, 9).Value = "Ticker"
    ws.Cells(1, 10).Value = "Yearly Change"
    ws.Cells(1, 11).Value = "Percent Change"
    ws.Cells(1, 12).Value = "Total Stock Volume"
    
    'Determine the last used row in Culumn A
    lastrow = ws.Cells(Rows.Count, 1).End(xlUp).Row
    
    'Loop through the rows by ticker names
    For i = 2 To lastrow
    
        'Check the value of the next cell to determine if it is different than that of the current cell
        If ws.Cells(i + 1, 1).Value <> ws.Cells(i, 1).Value Then
        
        'Set the ticker symbol
        tickersymbol = ws.Cells(i, 1).Value
        
        'Set the volume of stock
        tickervolume = tickervolume + ws.Cells(i, 7).Value
        
        'Print the ticker symbol in the summary table
        ws.Range("I" & summary_ticker_row).Value = tickersymbol
        
        'Print the trade volume for each ticker in the summary table
        ws.Range("L" & summary_ticker_row).Value = tickervolume
        
        'Collect closing price information
        closing_price = ws.Cells(i, 6).Value
        
        'Calculate yearly change from the opening price at the beginning of a given year to the closing price at the end of that year.
        yearly_change = (closing_price - opening_price)
        
        'Print the yearly change for each ticker in the summary table
        ws.Range("J" & summary_ticker_row).Value = yearly_change
        
        'Check for the non-divisibility condition when calculating the percent change
            If opening_price = 0 Then
                percent_change = 0
            Else
                percent_change = yearly_change / opening_price
            End If
            
        'Print the yearly change for each ticker in the summary table
        ws.Range("K" & summary_ticker_row).Value = percent_change
        ws.Range("K" & summary_ticker_row).NumberFormat = "0.00%"
        
        'Reset the row counter and add one
        summary_ticker_row = summary_ticker_row + 1
        
        'Reset the volume of trade to zero
        tickervolume = 0
        
        End If
             
        'Add the volume of the trade
        tickervolume = tickervolume + ws.Cells(i, 7).Value
        
        Next i
        
    'Highlight positive change in green and negative change in red
    lastrow_summary_table = ws.Cells(Rows.Count, 9).End(xlUp).Row
    
    'Color code yearly change
    For i = 2 To lastrow_summary_table
        If ws.Cells(i, 10).Value > 0 Then
        ws.Cells(i, 10).Interior.ColorIndex = 10
        Else
        ws.Cells(i, 10).Interior.ColorIndex = 3
        End If
        
    Next i
    
     'Color code percent change
    For i = 2 To lastrow_summary_table
        If ws.Cells(i, 11).Value > 0 Then
        ws.Cells(i, 11).Interior.ColorIndex = 10
        Else
        ws.Cells(i, 11).Interior.ColorIndex = 3
        End If
        
    Next i
        
    'Label cells for table
    ws.Cells(2, 15).Value = "Greatest % Increase"
    ws.Cells(3, 15).Value = "Greatest % Decrease"
    ws.Cells(4, 15).Value = "Greatest Total Volume"
    ws.Cells(1, 16).Value = "Ticker"
    ws.Cells(1, 17).Value = "Value"
    
    'Determine ticker name and corresponding values
    For j = 2 To lastrow_summary_table
    
    If ws.Cells(j, 11).Value = Application.WorksheetFunction.Max(ws.Range("K2:K" & lastrow_summary_table)) Then
    ws.Cells(2, 16).Value = ws.Cells(j, 9).Value
    ws.Cells(2, 17).Value = ws.Cells(j, 11).Value
    ws.Cells(2, 17).NumberFormat = "0.00%"
    
    ElseIf ws.Cells(j, 11).Value = Application.WorksheetFunction.Min(ws.Range("K2:K" & lastrow_summary_table)) Then
    ws.Cells(3, 16).Value = ws.Cells(j, 9).Value
    ws.Cells(3, 17).Value = ws.Cells(j, 11).Value
    ws.Cells(3, 17).NumberFormat = "0.00%"
    
    ElseIf ws.Cells(j, 12).Value = Application.WorksheetFunction.Max(ws.Range("L2:L" & lastrow_summary_table)) Then
    ws.Cells(4, 16).Value = ws.Cells(j, 9).Value
    ws.Cells(4, 17).Value = ws.Cells(j, 12).Value
    
    End If
    
    Next j
    
    Next ws
    
End Sub
